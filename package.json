{
  "name": "gatsby-plugin-s3",
  "version": "0.3.2",
  "description": "Enables you to deploy your gatsby site to a S3 bucket.",
  "main": "bin.js",
  "bin": "bin.js",
  "scripts": {
    "test": "jest",
    "prepublish": "npm run build",
    "build": "tsc --project .",
    "typecheck": "tsc --project . --noEmit",
    "develop": "babel --extensions=\".ts\" --out-dir=. src/ -w",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jariz/gatsby-plugin-s3.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "aws",
    "s3",
    "aws-sdk"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.10.5",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/es6-promisify": "^6.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.43",
    "@types/is-ci": "^1.1.0",
    "@types/klaw": "^2.1.1",
    "@types/mime": "^2.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/node-fetch": "^2.1.6",
    "@types/ora": "^3.0.0",
    "@types/stream-to-promise": "^2.2.0",
    "@types/yargs": "^12.0.2",
    "babel-jest": "^27.3.1",
    "es6-promisify": "^6.1.1",
    "gatsby": "^3.14.6",
    "glob": "^7.1.7",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "node-fetch": "^2.6.1",
    "tslint": "^5.18.0",
    "tslint-config-oberon": "^1.1.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "author": "Jari Zwarts <jarizw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jariz/gatsby-plugin-s3/issues"
  },
  "homepage": "https://github.com/jariz/gatsby-plugin-s3#readme",
  "dependencies": {
    "@types/async": "^2.4.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.2",
    "async": "^3.2.0",
    "aws-sdk": "^2.1018.0",
    "chalk": "^2.4.1",
    "fs-extra": "^8.1.0",
    "fs-posix": "^6.0.0",
    "inquirer": "^7.0.0",
    "is-ci": "^2.0.0",
    "jest": "^23.6.0",
    "klaw": "^3.0.0",
    "mime": "^2.4.0",
    "minimatch": "^3.0.4",
    "ora": "^3.0.0",
    "pretty-error": "^2.1.1",
    "stream-progressbar": "^1.1.1",
    "stream-to-promise": "^2.2.0",
    "yargs": "^12.0.5",
    "snyk": "^1.198.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "setupFiles": [
      "@babel/polyfill"
    ]
  },
  "lint-staged": {
    "*.ts?(x)": [
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "snyk": true
}
